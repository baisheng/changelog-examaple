{
  "name": "changelog-example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "generate-changelog": "ts-node scripts/changelogs/generate-changelog.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.24",
    "@types/klaw-sync": "^6.0.1",
    "@types/node": "^14.18.13",
    "concurrently": "^6.5.1",
    "conventional-changelog-core": "^4.2.4",
    "find": "^0.3.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "klaw-sync": "^6.0.0",
    "lint-staged": "^10.5.4",
    "pnpm": "^4.14.4",
    "prettier": "^2.6.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "4.1.5"
  },
  "resolutions": {
    "npm-packlist": "1.1.12"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        [
          "sentence-case"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@types/request": "^2.48.8",
    "fs-extra": "^10.1.0"
  }
}
